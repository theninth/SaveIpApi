{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Information"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "/tmp/testlogging/Log.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 31,
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Service"
    }
  },
  "ConnectionStrings": {
    "WebApiDatabase": "Data Source=/tmp/ipaddresses.db"
  },
  "AllowedHosts": "*"
}
